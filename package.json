  "scripts": {
    "dev": "pnpm dev:start",
    "dev:stop": "docker compose -f docker-compose.yaml down",
    "dev:start": "docker compose -f docker-compose.yaml up",
    "dev:rm": "docker compose -f docker-compose.yaml down --rmi local",
    "dev:setup": "pnpm dev:rm && sudo rm -Rf mongo_data && pnpm run dev:db:setup && pnpm run dev:hosts:setup",
    "dev:db:setup": "./init-scripts/setup-mongo-replica-set.sh",
    "dev:hosts:setup": "./init-scripts/init-hosts.sh",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/test/main",
    "tsc:check": "tsc --noEmit --project ./tsconfig.json",
    "lint": "pnpm tsc:check && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "jest": "cross-env NODE_ENV=$NENV jest --testTimeout=200000 --verbose",
    "jest:local": "NENV=jest pnpm jest",
    "jest:ci": "NENV=jest pnpm jest",
    "test": "pnpm run jest:local --projects .",
    "test:watch": "jest --watch",
    "test:cov": "pnpm run jest:local --runInBand --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "./test-e2e.sh local",
    "prepare": "husky install"
  },
